{% for color_and_scale in style_dict_list %}
<Style id="s{{ color_and_scale.id }}-{{ forloop.counter0 }}">
  <IconStyle>
    <scale>{{ color_and_scale.percentage }}</scale>
    <Icon>
      <href>/static/charts/img/{{color_and_scale.file_name}}</href>
    </Icon>
  </IconStyle>
</Style>
{% endfor %}